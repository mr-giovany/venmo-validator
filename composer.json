{
    "name": "sixshop/venmo-phone-validator",
    "description": "Venmo phone number validator",
    "keywords": [],
    "homepage": "https://sixshop.app",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "SixShop",
            "email": "me@sixshop.app"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/termwind": "^1.3",
        "laravel/pint": "^0.2.0",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/venmo"
    ]
}